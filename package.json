{
  "type": "commonjs",
  "name": "whispernetdb",
  "version": "1.0.0",
  "description": "The backend of WhisperNet, comes with the in-memory DB for the WhisperNet, and it is also an HTTP server that gets requests for operations for WhisperNet",
  "main": "server.js",
  "scripts": {
    "dev": "tsc -b && nodemon dist/server.js",
    "test": "tsc -b && cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "tsc -b && cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "tsc -b && cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:openHandles": "tsc -b && cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "build": "npm install && tsc",
    "start": "node dist/server.js"
  },
  "author": "Skizzy-Create (Kartik Aslia)",
  "license": "Skizzy",
  "dependencies": {
    "@types/jsonstream-next": "^3.0.3",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonstream-next": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.9",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.17.11",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}